{
	"name": "avisota/symfony",
	"description": "Avisota mailing system symfony bundle",
	"keywords": ["avisota", "email", "mail", "newsletter", "marketing", "symfony"],
	"homepage": "http://avisota.org",
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Tristan Lins",
			"email": "tristan.lins@bit3.de",
			"homepage": "http://bit3.de",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@bit3.de",
		"issues": "https://github.com/bit3/contao-avisota/issues",
		"forum": "https://community.contao.org/de/forumdisplay.php?121-Avisota",
		"wiki": "http://avisota.org",
		"irc": "irc://irc.freenode.org/contao.dev",
		"source": "https://github.com/bit3/contao-avisota"
	},
	"require": {
		"php": ">=5.3",
		"avisota/core": "~1",
		"symfony/dependency-injection": "~2.1",
		"symfony/http-kernel": "~2.1",
		"symfony/config": "~2.1",
		"symfony/yaml": "~2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"bit3/php-coding-standard": "2.*",
		"monolog/monolog": "~1",
		"symfony/event-dispatcher": "2.3.*"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-0": {
			"Avisota\\SymfonyBundle": "src/",
			"Avisota\\SymfonyBundle\\Test": "test/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	}
}
